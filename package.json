{
  "name": "steam-id-finder",
  "version": "0.1.5",
  "description": "A browser extension that calculates the IDs of given Steam users.",
  "license": "MIT",
  "repository": "avi12/steam-id-finder",
  "homepage": "https://avi12.com",
  "author": {
    "name": "avi12",
    "email": "avi6106@gmail.com"
  },
  "bugs": {
    "email": "avi6106@gmail.com"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-checkbox": "^0.20.0",
    "@material/mwc-formfield": "^0.20.0",
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@mdi/js": "^5.8.55",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup": "^2.34.2",
    "rollup-plugin-chrome-extension": "^3.5.1",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-empty-dir": "^1.0.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-zip": "^1.0.1",
    "sass": "^1.30.0",
    "set-json-version": "^1.0.4",
    "svelte": "^3.31.0",
    "svelte-materialify": "^0.3.4",
    "svelte-preprocess": "^4.6.1",
    "web-ext": "^4.3.0",
    "zip-self": "^0.2.5"
  },
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid",
    "svelteSortOrder": "scripts-markup-styles"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "pack": "web-ext build --overwrite-dest --artifacts-dir dist_packed --source-dir dist",
    "pack-self": "zip-self -o dist_packed/steam_id_finder-{version}-source.zip",
    "build-pack": "pnpm run build && pnpm run pack && pnpm run pack-stores && pnpm run pack-self",
    "set-version": "set-json-version -f package.json src/manifest.json -v",
    "validate": "svelte-check",
    "run-browser": "web-ext run --no-config-discovery --start-url https://steamcommunity.com/id/avi12 --source-dir dist --verbose --target",
    "run-firefox": "pnpm run-browser firefox-desktop",
    "run-chromium": "pnpm run-browser chromium"
  }
}
